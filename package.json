{
    "name": "clinic-app-root",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "start": "concurrently \"npm run start:server\" \"npm run start:ui\"",
        "start:server": "cd server && npm start",
        "start:ui": "cd ui && npm run dev",
        "install:all": "npm install && cd server && npm install && cd ui && npm install"
    },
    "devDependencies": {
        "concurrently": "^8.2.2"
    }
}